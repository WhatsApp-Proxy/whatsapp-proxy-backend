    "@types/fancy-log": "^2.0.0",
    "@types/http-errors": "^2.0.4",
    "@types/node": "^20.12.12",
    "prettier": "^2.8.2",
    "tslint": "^6.1.3"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.19.2",
